{
  "name": "react-shell",
  "version": "0.1.0",
  "author": "@aknorw",
  "repository": "https://github.com/aknorw/react-shell.git",
  "license": "MIT",
  "scripts": {
    "clean": "node_modules/.bin/rimraf dist",
    "pretty": "node_modules/.bin/prettier --config .prettierrc --write src/**/*.{js,jsx}",
    "lint": "node_modules/.bin/eslint --ext .jsx,.js src",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --updateSnapshot --detectOpenHandles",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack --mode development",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack --env.production --mode production --progress --colors"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "core-js": "3.2.0",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-hot-loader": "4.12.11"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "cross-env": "5.2.0",
    "dotenv": "8.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.3",
    "jest": "24.8.0",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "rimraf": "2.7.1",
    "terser-webpack-plugin": "1.4.1",
    "webpack": "4.39.1",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.8.0",
    "webpack-merge": "4.2.1"
  }
}
